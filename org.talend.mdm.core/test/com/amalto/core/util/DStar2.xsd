                    <xsd:appinfo source="X_Facet_FR">Valeurs possibles: pending, approved, terminated</xsd:appinfo>  
                    <xsd:appinfo source="X_Write">Demo_Manager</xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:all> 
    </xsd:complexType>  

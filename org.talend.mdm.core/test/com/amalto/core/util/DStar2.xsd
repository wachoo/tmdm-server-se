                    <xsd:appinfo source="X_Label_FR">Date d'entrée</xsd:appinfo>  
                    <xsd:appinfo source="X_Write">Demo_Manager</xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element>  
            <xsd:element maxOccurs="1" minOccurs="0" name="TermDate" nillable="false" type="xsd:date"> 

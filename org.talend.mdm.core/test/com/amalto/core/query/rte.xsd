                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_Visible_Rule">../participationSsy = "Oui" and
                                            ../calculPrescriptionSsy="Simple chronique"
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element maxOccurs="1" minOccurs="0" name="coefficientRSSSY" type="int3">

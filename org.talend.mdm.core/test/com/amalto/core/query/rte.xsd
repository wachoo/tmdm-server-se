            <xsd:appinfo source="X_Schematron">&lt;pattern name="UA" &gt; &lt;rule context="/"&gt;&lt;assert
                test="string-length(codeUa) &gt;=6 and string-length(codeUa)&amp;lt;=8 and
                (fn:compare(substring(codeUa,6,1),&amp;quot;T&amp;quot;)=0 or fn:compare(substring(codeUa,6,1),&amp;quot;H&amp;quot;)=0)or
                (controleCodeUA=&amp;quot;Non&amp;quot;) "&gt;&lt;![CDATA[[FR:Erreur sur le code de l'UA]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;/pattern&gt;</xsd:appinfo>
            <xsd:appinfo source="X_PrimaryKeyInfo">UniteAgregation/codeUa</xsd:appinfo>
            <xsd:appinfo source="X_PrimaryKeyInfo">UniteAgregation/nomUa</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_SRC</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="UniteAgregation">
            <xsd:selector xpath="."/>
            <xsd:field xpath="idUa"/>

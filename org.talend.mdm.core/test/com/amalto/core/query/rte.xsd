                                        <xsd:appinfo source="X_Visible_Rule">../participationSsy = "Oui"</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_Label_FR">SSy : Date de début</xsd:appinfo>
                                        <xsd:appinfo source="X_Display_Format_FR">%1$te/%1$tm/%1$tY</xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element maxOccurs="1" minOccurs="0" name="dateFinSsy" type="xsd:date">

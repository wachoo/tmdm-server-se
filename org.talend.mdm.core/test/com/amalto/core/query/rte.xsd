                                        <xsd:appinfo source="X_Label_FR">Participation aux SSy</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element maxOccurs="1" minOccurs="0" name="dateDebutSsy" type="xsd:date">

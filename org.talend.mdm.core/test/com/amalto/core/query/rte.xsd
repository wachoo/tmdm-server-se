                                        <xsd:appinfo source="X_Label_FR">Code SAP Vendeur</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_Description_FR">SAP_V_+ 4 premières lettres de
                                            codeActeur
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element maxOccurs="1" minOccurs="0" name="participationSsy" type="enumBool">

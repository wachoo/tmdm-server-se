                                    <xsd:annotation>
                                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_Label_FR">Volume max de RP consommée</xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element maxOccurs="1" minOccurs="0" name="commentaires" type="texte1000">
                                    <xsd:annotation>
                                        <xsd:appinfo source="X_Label_FR">Commentaires</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_Description_FR">Taille max(1000)</xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element maxOccurs="1" minOccurs="0" name="contratsRP">
                                    <xsd:annotation>
                                        <xsd:appinfo source="X_Label_FR">Consommateurs de RP à fournir</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_AutoExpand">false</xsd:appinfo>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="contratRP"
                                                         type="xsd:string">
                                                <xsd:annotation>
                                                    <xsd:appinfo source="X_ForeignKey">Contrat</xsd:appinfo>
                                                    <xsd:appinfo source="X_Label_FR">Contrat</xsd:appinfo>
                                                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                                    <xsd:appinfo source="X_ForeignKey_Filter">
                                                        Contrat/detailContrat/@xsi:type$$=$$&amp;quot;AP-RP&amp;quot;$$#
                                                    </xsd:appinfo>
                                                    <xsd:appinfo source="X_ForeignKeyInfo">Contrat/codeActeur
                                                    </xsd:appinfo>
                                                    <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                                                </xsd:annotation>
                                            </xsd:element>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>

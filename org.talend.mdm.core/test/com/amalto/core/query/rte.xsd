    <!-- Objet EDPrev -->
    <xsd:element name="Edprev" type="EdprevType">
        <xsd:annotation>
            <xsd:appinfo source="X_Label_FR">Edprev</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>


            <xsd:appinfo source="X_PrimaryKeyInfo">Edprev/codeEdprev</xsd:appinfo>
            <xsd:appinfo source="X_PrimaryKeyInfo">Edprev/nomEdprev</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_PhysicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_ReadOnly</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_DCO</xsd:appinfo>
            <xsd:appinfo source="X_Deny_LogicalDelete">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="CodeValidation" &gt; &lt;rule context="/"&gt;&lt;assert
                test="(( controleCodeEdprev=&amp;quot;Oui&amp;quot;) and string-length(codeEdprev) &gt;=6 and
                string-length(codeEdprev)&amp;lt;=8 and (fn:compare(substring(codeEdprev,6,1),&amp;quot;T&amp;quot;)=0
                or fn:compare(substring(codeEdprev,6,1),&amp;quot;H&amp;quot;)=0) and ( (fn:compare(
                substring(codeEdprev,7,2) ,concat(&amp;quot;C&amp;quot;,substring(useDonneurdOrdre,2,1)))=0) or
                ((fn:compare(&amp;quot; &amp;quot;,substring(codeEdprev,7,1))=0) and
                fn:matches(substring(codeEdprev,8,1),&amp;quot;[0-9]&amp;quot;)) or
                (string-length(substring(codeEdprev,7,2))=0) )) or (controleCodeEdprev=&amp;quot;Non&amp;quot;) or
                fn:empty(controleCodeEdprev)"&gt;&lt;![CDATA[[FR:Erreur sur le code de l'EDPrev]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;/pattern&gt;</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="DateFinValidation" &gt; &lt;rule context="/"&gt;&lt;assert
                test="translate(datedebutEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or
                translate(datefinEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or number(
                translate( datedebutEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt; number( translate(
                datefinEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La date de début de
                l'Edprev doit être inférieure à la date de fin de l'Edprev.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule
                context="/"&gt;&lt;assert test="translate(dateDebutSsyEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;)
                = &amp;quot;&amp;quot; or translate(dateFinSsyEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot;
                or number( translate( dateDebutSsyEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) ) &amp;lt;number(
                translate( dateFinSsyEdprev, &amp;quot;-&amp;quot;, &amp;quot;&amp;quot;) )"&gt;&lt;![CDATA[[FR: La date
                de début Ssy doit être inférieure à la date de fin Ssy.]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="SsyDateValidation" &gt; &lt;rule
                context="dateDebutSsyEdprev"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;)
                = &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;=
                number(translate(../datedebutEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and
                (translate(../datefinEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or
                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;
                number(translate(../datefinEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)))"&gt;&lt;![CDATA[[FR:La
                date début Ssy doit être comprise entre la date début Edprev (inclusif) et la date fin Edprev
                (exclusif).]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="dateFinSsyEdprev"&gt;&lt;assert
                test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or
                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;=
                number(translate(../datedebutEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and
                (translate(../datefinEdprev,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;) = &amp;quot;&amp;quot; or
                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;=
                number(translate(../datefinEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)))"&gt;&lt;![CDATA[[FR:La
                date fin Ssy doit être comprise entre la date début Edprev (inclusif) et la date fin Edprev
                (inclusif).]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="Edprev">
            <xsd:selector xpath="."/>
            <xsd:field xpath="idEdprev"/>
        </xsd:unique>
    </xsd:element>
    <xsd:complexType name="EdprevType">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="codeEdprev" type="texte8">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Code</xsd:appinfo>
                    <xsd:appinfo source="X_Description_FR">Règle de codification : cf. Règles MA</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
                    <xsd:appinfo source="X_Workflow">RAP_DCO#WorkflowEdprev_1.0#Writable</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="nomEdprev" type="texte25">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Description_FR">Libellé de l'EDPrev</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Nom détaillé</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="controleCodeEdprev" type="enumBool">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Default_Value_Rule">"Oui"</xsd:appinfo>
                    <xsd:appinfo source="X_Description_FR">ex: EDPrev influencé ZZZSCR</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Contrôler le code de l'EDPrev ?</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="statutEdprev" type="TypeStatutEntite">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Statut de l'EDPrev</xsd:appinfo>
                    <xsd:appinfo source="X_Default_Value_Rule">"Brouillon"</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
                    <xsd:appinfo source="X_Workflow">RAP_DCO#WorkflowEdprev_1.0#Hidden</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="demandeValidation" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Demande de validation</xsd:appinfo>
                    <xsd:appinfo source="X_Visible_Rule">../statutEdprev = "Brouillon"</xsd:appinfo>
                    <xsd:appinfo source="X_Default_Value_Rule">false</xsd:appinfo>
                    <xsd:appinfo source="X_Workflow">RAP_DCO#WorkflowEdprev_1.0#Hidden</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="demandeDiffusion" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Demande de diffusion</xsd:appinfo>
                    <xsd:appinfo source="X_Default_Value_Rule">false</xsd:appinfo>
                    <xsd:appinfo source="X_Visible_Rule">../statutEdprev = "Validée"</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="numeroContrat" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_ForeignKey">Contrat</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Code Acteur</xsd:appinfo>


                    <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Contrat/codeActeur</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Contrat/numeroContratExterne</xsd:appinfo>
                    <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="interlocuteurClientOperationnel" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_ForeignKey">Interlocuteur/idGrc</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>


                    <xsd:appinfo source="X_Label_FR">Interlocuteur Opérationnel TR</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_ReadOnly</xsd:appinfo>
                    <xsd:appinfo source="X_Deny_Create">RAP_DCO</xsd:appinfo>


                    <xsd:appinfo source="X_ForeignKeyInfo">Interlocuteur/codeActeur</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Interlocuteur/nom</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Interlocuteur/prenom</xsd:appinfo>
                    <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

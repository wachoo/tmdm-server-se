                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_Label_FR">Fournisseur RP</xsd:appinfo>
                                        <xsd:appinfo source="X_Description_FR">Vente et achat de RP à l’étranger
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                                <xsd:element maxOccurs="1" minOccurs="0" name="consommateurRP" type="enumBool">
                                    <xsd:annotation>
                                        <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                        <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                                        <xsd:appinfo source="X_Label_FR">Consommateur de RP</xsd:appinfo>
                                        <xsd:appinfo source="X_Description_FR">Vente et achat de RP à l’étranger
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>

                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="typeTransaction" type="typeTransaction">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Type</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_ReadOnly</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_DCO</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="dateDebutTransaction" type="xsd:date">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Date de début</xsd:appinfo>
                    <xsd:appinfo source="X_Description_FR">Date de début de validité à la sauvegarde, si l'utilisateur
                        n'a rien renseigné.
                    </xsd:appinfo>

                    <xsd:appinfo source="X_Display_Format_FR">%1$te/%1$tm/%1$tY</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="dateFinTransaction" type="xsd:date">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Date de fin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>

                    <xsd:appinfo source="X_Display_Format_FR">%1$te/%1$tm/%1$tY</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="GRT" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">GRT</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKey">Grt/idGrt</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Grt/nomGrt</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Grt/frontiereGrt</xsd:appinfo>
                    <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="PerimetreRE" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_ForeignKey">Contrat/numeroContrat</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKey_Filter">$CFFP:detailContrat[@xsi:type=&amp;quot;AP-RE&amp;quot;]
                    </xsd:appinfo>


                    <xsd:appinfo source="X_Label_FR">Responsable d'équilibre</xsd:appinfo>
                    <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="commentaireTransaction" type="texte1000">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Description_FR">Taille max (1000)</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Commentaires</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

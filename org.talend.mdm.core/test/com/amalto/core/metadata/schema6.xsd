            <xsd:element maxOccurs="1" minOccurs="0" name="adresseCp" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">CP</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="adresseLocalite" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Ville</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="adresseCodeBp" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">BP</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="adressePays" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Pays</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="telephone" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Telephone</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_DCO</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_ReadOnly</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="telecopie" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Telecopie</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_SRC</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_DCO</xsd:appinfo>
                    <xsd:appinfo source="X_Hide">RAP_ReadOnly</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="tvaIntracommunautaire" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">TVA Intracommunautaire</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="siret" type="xsd:int">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">SIRET</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="rcs" type="xsd:int">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">RCS</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="libelleNaf" type="xsd:string">

    <!-- Objet EDP -->
    <xsd:element name="Edp" type="EdpType">
        <xsd:annotation>
            <xsd:appinfo source="X_Label_FR">Edp</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="SsyDateValidation" &gt;
                &lt;rule context="dateDebutSsyEdp"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;)
                = &amp;quot;&amp;quot;

                or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;=

                number(translate(../datedebutEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and

                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;

                number(translate(../datefinEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La date
                debut Ssy doit ?tre comprise

                entre la date debut Edp (inclusif) et la date fin Edp (exclusif)]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;rule context="dateFinSsyEdp"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;)
                = &amp;quot;&amp;quot; or

                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;=

                number(translate(../datedebutEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and

                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;=

                number(translate(../datefinEdp,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La date
                fin Ssy doit ?tre comprise

                entre la date debut Edp (inclusif) et la date fin Edp (inclusif)]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;/pattern&gt;
            </xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="CodeValidation" &gt;
                &lt;rule context="/"&gt;&lt;assert test="string-length(codeEdp) &gt;=6 and string-length(codeEdp)&amp;lt;=8
                and

                fn:matches(substring(codeEdp,1,5),&amp;quot;[A-Za-z0-9\s]*&amp;quot;)
                and((fn:compare(substring(codeEdp,6,1),&amp;quot;T&amp;quot;)=0

                or (fn:compare(substring(codeEdp,6,1),&amp;quot;H&amp;quot;)=0)))and( (fn:compare(
                substring(codeEdp,7,2)

                ,concat(&amp;quot;C&amp;quot;,substring(typeEdp/UrseTelereleve,2,1)))=0) or ((fn:compare(&amp;quot;

                &amp;quot;,substring(codeEdp,7,1))=0) and fn:matches(substring(codeEdp,8,1),&amp;quot;[0-9]&amp;quot;))
                or

                (string-length(substring(codeEdp,7,2))=0) )"&gt;&lt;![CDATA[[FR: Erreur sur le code de l'EDP]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;/pattern&gt;
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="Edp">
            <xsd:selector xpath="."/>
            <xsd:field xpath="codeEdp"/>
        </xsd:unique>
    </xsd:element>
    <xsd:complexType name="EdpType">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="codeEdp" type="texte8">
                <xsd:annotation>

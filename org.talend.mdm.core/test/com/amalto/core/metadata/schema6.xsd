                (fn:compare( substring(codeEda,7,2) ,concat(&amp;quot;C&amp;quot;,substring(useDonneurdOrdre,2,1)))=0)
                or ((fn:compare(&amp;quot;

                &amp;quot;,substring(codeEda,7,1))=0) and fn:matches(substring(codeEda,8,1),&amp;quot;[0-9]&amp;quot;))
                or

                (string-length(substring(codeEda,7,2))=0) )"&gt;&lt;![CDATA[[FR: Erreur sur le codeEda]]]&gt;&lt;/assert&gt;&lt;/rule&gt;

                &lt;/pattern&gt;</xsd:appinfo>

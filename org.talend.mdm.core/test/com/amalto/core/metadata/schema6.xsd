                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="contrats">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Contrats souscrits</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="contrat" type="xsd:string">
                            <xsd:annotation>
                                <xsd:appinfo source="X_ForeignKey">Contrat/numeroContrat</xsd:appinfo>
                                <xsd:appinfo source="X_Label_FR">Contrat</xsd:appinfo>
                                <xsd:appinfo source="X_ForeignKeyInfo">Contrat/numeroContrat</xsd:appinfo>
                                <xsd:appinfo source="X_ForeignKeyInfo">Contrat/numeroContratExterne</xsd:appinfo>
                                <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                                <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                                <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <!-- Objet CONTRAT -->
    <xsd:element name="Contrat" type="ContratType">
        <xsd:annotation>
            <xsd:appinfo source="X_Label_FR">Contrat</xsd:appinfo>


            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="Contrat">
            <xsd:selector xpath="."/>
            <xsd:field xpath="numeroContrat"/>
        </xsd:unique>
    </xsd:element>
    <xsd:complexType name="ContratType">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="numeroContrat" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Label_FR">Numero de Contrat GRC</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="numeroContratExterne" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Numero Accord de Participation</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="dateSignatureContrat" type="xsd:date">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Date de signature du contrat</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="dateDebutPrevueContrat" type="xsd:date">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Date de debut prevue</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="dateDebutEffectiveContrat" type="xsd:date">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Date de debut effective</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="dateFinContrat" type="xsd:date">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Date de fin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="commentaires" type="texte1000">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Commentaires</xsd:appinfo>
                    <xsd:appinfo source="X_Description_FR">Taille max(1000)</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="interlocuteurClientContractuel" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Interlocuteur Client du Contrat</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKey">Interlocuteur/idGrc</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Interlocuteur/nom</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Interlocuteur/prenom</xsd:appinfo>
                    <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Visible_Rule">false</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="interlocuteurRteContractuel" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Interlocuteur RTE du Contrat</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKey">Interlocuteur/idGrc</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Interlocuteur/nom</xsd:appinfo>
                    <xsd:appinfo source="X_ForeignKeyInfo">Interlocuteur/prenom</xsd:appinfo>
                    <xsd:appinfo source="X_Retrieve_FKinfos">true</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Visible_Rule">false</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="detailContrat" type="detailContrat">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Detail du Contrat</xsd:appinfo>


                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType abstract="true" name="detailContrat">
        <xsd:sequence/>
    </xsd:complexType>

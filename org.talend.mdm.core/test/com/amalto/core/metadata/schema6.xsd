    <!-- Objet EDA -->
    <xsd:element name="Eda" type="EdaType">
        <xsd:annotation>
            <xsd:appinfo source="X_Label_FR">EDA</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="Eda" &gt; &lt;rule context="/"&gt;&lt;assert
                test="string-length(codeEda)

                &gt;=6 and string-length(codeEda)&amp;lt;=8 and fn:matches(substring(codeEda,1,5),&amp;quot;[A-Za-z0-9\s]*&amp;quot;)


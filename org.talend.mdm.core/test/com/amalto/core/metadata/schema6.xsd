    <!-- Objet EDPrev -->
    <xsd:element name="Edprev" type="EdprevType">
        <xsd:annotation>

            <xsd:appinfo source="X_Label_FR">Edprev</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="CodeValidation" &gt;
                &lt;rule context="/"&gt;&lt;assert test="string-length(codeEdprev) &gt;=6 and string-length(codeEdprev)&amp;lt;=8
                and

                fn:matches(substring(codeEdprev,1,5),&amp;quot;[A-Za-z0-9\s]*&amp;quot;)

                and((fn:compare(substring(codeEdprev,6,1),&amp;quot;T&amp;quot;)=0 or

                (fn:compare(substring(codeEdprev,6,1),&amp;quot;H&amp;quot;)=0)))and( (fn:compare(
                substring(codeEdprev,7,2)

                ,concat(&amp;quot;C&amp;quot;,substring(typeEdprev/UrseTelereleve,2,1)))=0) or ((fn:compare(&amp;quot;

                &amp;quot;,substring(codeEdprev,7,1))=0) and fn:matches(substring(codeEdprev,8,1),&amp;quot;[0-9]&amp;quot;))
                or

                (string-length(substring(codeEdprev,7,2))=0) )"&gt;&lt;![CDATA[[FR: Erreur sur le code de l' EDPrev]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;/pattern&gt;
            </xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="SSyDateValidation" &gt;
                &lt;rule context="dateDebutSsyEdprev"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;)
                =

                &amp;quot;&amp;quot; or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;=

                number(translate(../datedebutEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and

                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;

                number(translate(../datefinEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La
                date debut Ssy doit ?tre comprise

                entre la date debut Edprev (inclusif) et la date fin Edprev (exclusif)]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;rule context="dateFinSsyEdprev"&gt;&lt;assert test="translate(.,&amp;quot; &amp;quot;,&amp;quot;&amp;quot;)
                = &amp;quot;&amp;quot;

                or number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &gt;=

                number(translate(../datedebutEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) and

                number(translate(.,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;)) &amp;lt;=

                number(translate(../datefinEdprev,&amp;quot;-&amp;quot;,&amp;quot;&amp;quot;))"&gt;&lt;![CDATA[[FR:La
                date fin Ssy doit ?tre comprise

                entre la date debut Edprev (inclusif) et la date fin Edprev (inclusif)]]]&gt;&lt;/assert&gt;&lt;/rule&gt;
                &lt;/pattern&gt;
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="Edprev">
            <xsd:selector xpath="."/>
            <xsd:field xpath="codeEdprev"/>
        </xsd:unique>
    </xsd:element>
    <xsd:complexType name="EdprevType">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" name="codeEdprev" type="texte8">
                <xsd:annotation>
                    <xsd:appinfo source="X_Label_FR">Code</xsd:appinfo>
                    <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
                    <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>

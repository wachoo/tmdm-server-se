    <!-- Objet UniteAgregation -->
    <xsd:element name="UniteAgregation" type="UniteAgregationType">
        <xsd:annotation>
            <xsd:appinfo source="X_Label_FR">Unite Agregation</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>
            <xsd:appinfo source="X_Write">RAP_SRC</xsd:appinfo>
            <xsd:appinfo source="X_Schematron">&lt;pattern name="UA" &gt; &lt;rule context="/"&gt;&lt;assert
                test="string-length(codeUa)

                &gt;=6 and string-length(codeUa)&amp;lt;=8 and fn:matches(substring(codeUa,1,5),&amp;quot;[A-Za-z0-9\s]*&amp;quot;)

                and((fn:compare(substring(codeUa,6,1),&amp;quot;T&amp;quot;)=0 or

                (fn:compare(substring(codeUa,6,1),&amp;quot;H&amp;quot;)=0)))and(((fn:compare(&amp;quot; &amp;quot;,substring(codeUa,7,1))=0)
                and

                fn:matches(substring(codeUa,8,1),&amp;quot;[0-9]&amp;quot;)) or (string-length(substring(codeUa,7,2))=0)
                )"&gt;&lt;![CDATA[[FR:Erreur

                sur le code de l'UA]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>
        </xsd:annotation>
        <xsd:unique name="UniteAgregation">
            <xsd:selector xpath="."/>
            <xsd:field xpath="codeUa"/>
        </xsd:unique>
    </xsd:element>

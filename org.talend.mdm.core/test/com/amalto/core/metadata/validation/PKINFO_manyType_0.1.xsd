<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://www.w3.org/2001/XMLSchema"/>
    <xsd:complexType name="EType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" name="name2" type="xsd:string"/>
        </xsd:all>
    </xsd:complexType>
    <xsd:element name="Root1">
        <xsd:annotation>
            <xsd:appinfo source="X_PrimaryKeyInfo">Root1/eName/name</xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="1" name="id" type="xsd:string"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="eName" type="EType"/>
            </xsd:all>
        </xsd:complexType>
        <xsd:unique name="Root1">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>
    <xsd:element name="Root2">
        <xsd:annotation>
            <xsd:appinfo source="X_PrimaryKeyInfo">Root2/eName/name</xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="1" name="id" type="xsd:string"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="eName" type="EType"/>
            </xsd:all>
        </xsd:complexType>
        <xsd:unique name="Root2">
            <xsd:selector xpath="."/>
            <xsd:field xpath="id"/>
        </xsd:unique>
    </xsd:element>
</xsd:schema>

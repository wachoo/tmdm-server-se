  <xsd:element name="DemandeMatiere" type="DemandeMatiereType"> 
    <xsd:annotation> 
      <xsd:appinfo source="X_Description_FR">Une matière caractérise les produits manipulés qui jouent un rôle dans les activités de Veolia Propreté (qu’ils soient considérés comme Déchets, Matières Premières où Réactifs)</xsd:appinfo>  
      <xsd:appinfo source="X_Schematron">&lt;pattern name="ValidationMatiere" &gt; &lt;rule context="/"&gt;&lt;assert test="fn:not(fn:contains(&amp;quot;0123456789&amp;quot;,fn:substring(CodeMatiere,1,1)))"&gt;&lt;![CDATA[[FR:Le code de la matière commence obligatoirement par une lettre]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;rule context="/"&gt;&lt;assert test="(fn:stringLength(MotsCles)=0)or (fn:stringLength(MotsCles)&gt;0 and fn:matches(MotsCles,&amp;quot;(.+;)+&amp;quot;))"&gt;&lt;![CDATA[[FR:Chaque mot clé est terminé par un point virgule]]]&gt;&lt;/assert&gt;&lt;/rule&gt; &lt;/pattern&gt;</xsd:appinfo>  
      <xsd:appinfo source="X_Label_FR">Demande de matière</xsd:appinfo>  
      <xsd:appinfo source="X_Write">System_Interactive</xsd:appinfo> 
    </xsd:annotation>  
    <xsd:unique name="DemandeMatiere"> 
      <xsd:selector xpath="."/>  
      <xsd:field xpath="CodeMatiere"/> 
    </xsd:unique> 
  </xsd:element>  
  <xsd:simpleType name="AUTO_INCREMENT"> 
    <xsd:restriction base="xsd:string"/> 
  </xsd:simpleType>  

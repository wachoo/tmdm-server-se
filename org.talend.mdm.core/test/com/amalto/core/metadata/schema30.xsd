          <xsd:appinfo source="X_Write">System_Admin</xsd:appinfo>  
          <xsd:appinfo source="X_Write">System_Interactive</xsd:appinfo>  
          <xsd:appinfo source="X_Write">AdministrateurReferentiel</xsd:appinfo> 
